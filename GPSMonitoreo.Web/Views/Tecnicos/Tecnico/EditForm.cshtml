
@{
    Layout = "_AppLayoutTabbedForm3";

    var layoutModel = new GPSMonitoreo.Web.ViewModels.AppLayoutForm(this.ViewContext);

    layoutModel.Title = "TECNICOS::TECNICO";

    ViewData["layoutModel"] = layoutModel;

}

@*si se puede*@
<script script-manager-add="commonform" script-manager-key="form_init">
    layout.addFormInitializer(function(form){
        var f = form.$form[0];
       
        form.add([f.Alterno, f.Prueba1, f.Prueba2], 'jqxInput');
    });


   
</script>


<formsection form-manager="commonform" form-manager-section="section0">
  
  <div id="GroupBoxServiceType" style="font-family: Verdana Arial; font-size: 12px; width: 400px;">
      <fieldset style="width:200px">
          <legend>Tipo de Servicio</legend>

          <div id="@(layoutModel.FormId)_jqxRadioButton" field_name="2">
              Contacto
          </div>
          <div id="@(layoutModel.FormId)_jqxRadioButton2" field_name="1">
              Garantia
          </div>
          <div id="@(layoutModel.FormId)_jqxRadioButton3" field_name="3">
              Ocacional
          </div>
      </fieldset>
  </div>
</formsection>



<formsection form-manager="commonform" form-manager-section="section6">

    <div id="GroupBoxClients" style="font-family: Verdana Arial; font-size: 12px; width: 400px;">
        <fieldset style="width:200px">
            <legend>Cliente</legend>

            <div id="@(layoutModel.FormId)_jqxRadioButton4" field_name="4">
                Nuevo
            </div>
            <div id="@(layoutModel.FormId)_jqxRadioButton5" field_name="5">
                Existente
            </div>
            
        </fieldset>
    </div>





</formsection>
<formsection form-manager="commonform" form-manager-section="section1">
        <tr>
            <td>Código Alterno</td>
            <td><input name="Alterno" maxlength="10" /></td>
        </tr>
</formsection>


<formsection form-manager="commonform" form-manager-section="section2">
    <tr>
        <td>prueba1</td>
